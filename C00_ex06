#include <unistd.h>

void	ft_print_comb2(void)
{
	int		a;
	int		b;
	char	d[4];

	a = 0;
	while (a <= 98)
	{
		b = a + 1;
		while (b <= 99)
		{
			d[0] = a / 10 + '0';
			d[1] = a % 10 + '0';
			d[2] = b / 10 + '0';
			d[3] = b % 10 + '0';
			write (1, &d[0], 1);
			write (1, &d[1], 1);
			write (1, " ", 1);
			write (1, &d[2], 1);
			write (1, &d[3], 1);
			if (a != 98 || b != 99)
				write (1, ", ", 2);
		b++;
		}
	a++;
	}
}
